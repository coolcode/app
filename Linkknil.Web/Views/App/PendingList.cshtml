@(
 Html.Grid<AppViewModel>().Columns(c => {
     c.Column(x => x.Name).Header("应用名称").Sortable(true).Template(@<text> @Html.DialogLink(item.Name, "PendingDetails", new { id = item.Id })</text>);
     c.Column(x => x.Dev).Header("开发者").Sortable(true);
     c.Column(x => x.CreateTime).Header("创建时间").Sortable(true);
     c.Column().Header("操作").Width(100).Align(Alignment.Center).Template(@<text>
              @Html.AjaxLink("通过", "Pass", new { id = item.Id })
          </text>);
      })
      .Sort(defaultSort: "CreateTime")
)