@{
    Layout = "~/Views/Shared/_Default.cshtml";
}
@section css{
	<link href="../../Content/js/jquery/plugins/fancybox/jquery.fancybox.css" rel="stylesheet" type="text/css" />
	@RenderSection("css", false) 
}

@section script{
	<script src="../../Content/js/site/site.js" type="text/javascript"></script>
	<script src="../../Content/js/jquery/plugins/fancybox/jquery.fancybox.js" type="text/javascript"></script>
	@if (IsSectionDefined("script")) {@RenderSection("script") }
} 

@section body{
<body id="site-body">
	<div id="body-wrapper"> <!-- Wrapper for the radial gradient background -->
		
		<div id="sidebar"><div id="sidebar-wrapper"> <!-- Sidebar with logo and menu --> 
			<h1 id="sidebar-title"><a href="/">Linkknil</a></h1>
		  
			<!-- Logo (221px wide) 
			<a href="/"><img src="../../Content/images/logo.png" alt="logo" /></a>-->
		  
			<!-- Sidebar Profile links -->
			<div id="profile-links">
				@Html.Partial("_LogOnPartial") 
			</div>        
			 
			@Html.Action("Menu") 
			
		</div></div> <!-- End #sidebar -->
		
		<div id="main-content"> <!-- Main Content Section with everything -->
			
			<noscript> <!-- Show a notification if the user has disabled javascript -->
				<div class="notification error png_bg">
					<div>
						Javascript is disabled or is not supported by your browser. Please <a href="http://browsehappy.com/" title="Upgrade to a better browser">upgrade</a> your browser or <a href="http://www.google.com/support/bin/answer.py?answer=23852" title="Enable Javascript in your browser">enable</a> Javascript to navigate the interface properly.
					Download From <a href="http://www.exet.tk">exet.tk</a></div>
				</div>
			</noscript>
			
			 		 
			@RenderBody()

			<div class="clear"></div>			
			  
			
			@Html.Partial("_Footer")  
			
		</div> <!-- End #main-content -->
		
	</div>
</body>
}