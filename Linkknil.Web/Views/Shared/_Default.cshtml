<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<meta http-equiv="content-type" content="text/html;charset=UTF-8" />
	<meta name="keywords" content="CoolCode,Open Source,ASP.NET MVC,Linq To SQL,IoC" />
	<meta name="description" content="CoolCode" />
	<link rel="icon" href="/favicon.ico" type="image/x-icon" />
	<link rel="shortcut icon" href="/favicon.ico" type="image/x-icon" />
	@*<link rel="apple-touch-icon" href="/apple-touch-icon.png" />*@
	<title>@ViewBag.Title</title>
	<link href="../../Content/css/default/reset.css" rel="stylesheet" type="text/css" />
	<link href="../../Content/css/default/site.css" rel="stylesheet" type="text/css" />
	<link href="../../Content/css/default/invalid.css" rel="stylesheet" type="text/css" />
	<link href="../../Content/js/jquery/plugins/datepicker/skin/WdatePicker.css" rel="stylesheet" type="text/css" />
	@if (IsSectionDefined("css")) {@RenderSection("css") }
</head>
@if (IsSectionDefined("body")){
	@RenderSection("body") 
}
else { 
	@RenderBody()
}
<div id="status">
	@Html.Partial("Controls/_Loading")
</div>
<script src="../../Content/js/jquery/jquery.js" type="text/javascript"></script>
<script src="../../Content/js/jquery/plugins/validate/jquery.validate.js" type="text/javascript"></script>
<script src="../../Content/js/jquery/plugins/validate/jquery.validate.zh-cn.js" type="text/javascript"></script>
<script src="../../Content/js/core/core.js" type="text/javascript"></script>
<script src="../../Content/js/core/grid.js" type="text/javascript"></script>
<script src="../../Content/js/jquery/plugins/datepicker/WdatePicker.js" type="text/javascript"></script>
@if (IsSectionDefined("script")){@RenderSection("script") }
</html>
